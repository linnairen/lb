page>view {
    background: none;
}

.coupon_list {
    width: 710rpx;
    margin: 0 auto;
}

.coupon_item {
    width: 100%;
    margin-bottom: 20rpx;
    background: #fff;
    position: relative;
    display: flex;
    align-items: center;
}

.coupon_item::before {
    content: '';
    position: absolute;
    width: 20rpx;
    height: 20rpx;
    top: 50%;
    left: 0;
    margin: -10rpx 0 0 -10rpx;
    background: #f1f1f1;
    border-radius: 50%;
}

.coupon_item::after {
    content: '';
    position: absolute;
    width: 20rpx;
    height: 20rpx;
    top: 50%;
    right: 0;
    margin: -10rpx -10rpx 0 0;
    background: #f1f1f1;
    border-radius: 50%;
}

.coupon_left {
    height: 94rpx;
    border-right: 1rpx dashed #c8c8c8;
    display: flex;
    width: 145rpx;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #F23939;
    font-family: 'PingFangSC-Medium';
}

.coupon_left text {
    font-size: 48rpx;
    margin-left: 8rpx;
}

.coupon_right {
    height: 200rpx;
    flex: 1;
    display: flex;
    align-items: center;
}

.detail {
    flex: 1;
    font-size: 26rpx;
    color: #888;
    line-height: 38rpx;
    height: 100%;
    padding-top: 28rpx;
    padding-left: 40rpx;
    box-sizing: border-box;
}

.detail .t {
    text-align: left;
    font-size: 32rpx;
    font-family: 'PingFangSC-Medium';
    line-height: 60rpx;
    color: #444;
}

.coupon_right button {
    width: 124rpx;
    height: 52rpx;
    color: #fff;
    background: #F23939;
    border-radius: 26rpx;
    text-align: center;
    line-height: 50rpx;
    margin-right: 30rpx;
    font-size: 28rpx;
    padding: 0;
    border: none;
}

.coupon_right button::after {
    border: none;
}

.coupon_right button[disabled] {
    background: #a9a9a9;
    color: #fff;
}

.tl {
    padding: 40rpx 0;
}

.t {
    color: #444444;
    font-size: 30rpx;
    text-align: center;
    flex: 1;
}

.i {
    display: block;
    width: 200rpx;
    border-bottom: 1rpx solid #A9A9A9;
    font-size: 0;
    height: 0;
}

.msgBox {
    width: 100%;
    background: #E8E8E8;
}

.msgTit {
    display: flex;
    align-items: center;
    height: 46rpx;
    line-height: 46rpx;
    font-size: 22rpx;
    color: #F23939;
}

.msgCont {
    padding: 10rpx 0;
    font-size: 20rpx;
    color: #888888;
}

.msgContText {
    padding: 10rxp 0;
    height: 28rpx;
    text-indent: 20rpx;
}

.click {
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.msgImg {
    transform: rotate(-180deg);
}

.shadeMain {
    width: 566rpx;
    height: 650rpx;
}

.shadeBg {
    position: absolute;
    top: 0;
    left: 0;
    width: 566rpx;
    height: 650rpx;
    z-index: -1;
}

.shadeTit {
    position: absolute;
    top: 356rpx;
    font-size: 28rpx;
    color: #671507;
    width: 100%;
    text-align: center;
}

.shadeCon {
    font-size: 28rpx;
    color: #671507;
    padding: 400rpx 33rpx 0 33rpx;
}